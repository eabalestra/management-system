<div id="<%= dom_id supplier %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= supplier.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= supplier.phone %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Dir:</strong>
    <%= supplier.dir %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= supplier.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">City:</strong>
    <%= supplier.city %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Website:</strong>
    <%= supplier.website %>
  </p>

</div>
